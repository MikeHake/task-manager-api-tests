{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Users",
    "description":"List of users",
    "type": "object",
    "required": ["url", "items"],
    "additionalProperties":false,
    "properties": {
        "url": {
            "description": "Self URL",
            "type": "string"
        },
        "items": {
            "title": "User",
            "description":"A single user within a list of users",
            "type": "array",
            "required": ["name", "roles"],
            "additionalProperties":false,
            "properties": {
                "name": {
                    "description": "The unique user name",
                    "type": "string"
                },
                "roles": {
                    "description":"List of roles this user has",
                    "type": "array",
                    "items":{
                       "type":"string",
                        "enum":["ADMIN", "PROJECT_ADMIN", "USER"]
                    }
            }
        }     
    }
     }
    
}